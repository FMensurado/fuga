fib = method(
    (n), fib(n, 0, 1)
    (0, a, b), a
    (n, a, b), fib(n-1, b, a+b)
)

START = 0
END   = 40 

loop(n) =>
    if( n > END
        void
        do( print(fib(n))
            loop(n+1) ) )

loop(START)

